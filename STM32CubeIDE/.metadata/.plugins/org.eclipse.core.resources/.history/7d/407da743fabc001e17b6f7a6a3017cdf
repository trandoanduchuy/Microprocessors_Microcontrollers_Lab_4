#include "led.h"

BLINK_RED_LED_STATUS blink_red_led_status = BLINK_RED_LED_INIT;

void BlinkRedLed(void)
{
	switch(blink_red_led_status)
	{
	case BLINK_RED_LED_INIT:
		HAL_GPIO_WritePin(LED_RED_GPIO_Port, LED_RED_Pin, GPIO_PIN_SET);
		break;
	case LED_ON:
		break;
	case LED_OFF:
		break;
	}
}

